{
	"lists.session.directory": "$HOME/.nconf/sessions",
	"list.source.files.args": [
		"--hidden",
		"--files"
	],
	"diagnostic.errorSign": "âœ–",
	"diagnostic.warningSign": "âš ",
	"diagnostic.infoSign": "â„¹",
	"diagnostic.hintSign": "âž¤",
	"suggest.noselect": false,
	"suggest.echodocSupport": true,
	"codeLens.enable": true,
	"signature.enable": true,
	"suggest.preferCompleteThanJumpPlaceholder": true,
	"typescript.preferences.quoteStyle": "single",
	"typescript.preferences.importModuleSpecifier": "auto",
	"tsserver.implicitProjectConfig.experimentalDecorators": true,
	"html.suggest.angular1": false,
	"html.suggest.ionic": false,
	"html.suggest.html5": true,
	"python.venvFolders": [
		"envs",
		".pyenv",
		".direnv",
		"~/.local/share/virtualenvs",
		"~/.cache/pypoetry/virtualenvs"
	],
	"python.workspaceSymbols.exclusionPatterns": [],
	"python.jediEnabled": false,
	"python.linting.pylintEnabled": true,
	"python.linting.pylintArgs": [
		"--load-plugins",
		"pylint_django"
	],
	"python.formatting.blackPath": "~/.local/bin/black",
	"python.linting.mypyEnabled": true,
	"python.formatting.provider": "black",
	"python.sortImports.path": "/home/calev/.local/bin/isort",
	"git.addedSign.text": "â–Ž",
	"git.changedSign.text": "â–Ž",
	"git.removedSign.text": "â–¶",
	"git.changeRemovedSign.text": "â–¶",
	"git.topRemovedSign.text": "â—¥",
	"git.addedSign.hlGroup": "GitGutterAdd",
	"git.changedSign.hlGroup": "GitGutterChange",
	"git.removedSign.hlGroup": "GitGutterDelete",
	"git.topRemovedSign.hlGroup": "GitGutterDelete",
	"git.changeRemovedSign.hlGroup": "GitGutterChangeDelete",
	"explorer.file.columns": [
		"git",
		"selection",
		"diagnosticError",
		"indent",
		"icon",
		"filename",
		"size"
	],
	"explorer.file.showHiddenFiles": true,
	"explorer.icon.enableNerdfont": true,
	"explorer.width": 40,
	"explorer.keyMappings": {
		"<right>": "expand",
		"<left>": "shrink"
	},
	"explorer.quitOnOpen": true,
	"coc.preferences.formatOnSaveFiletypes": [
		"css",
		"Markdown"
	],
	"list.normalMappings": {
		"t": "action:tabe",
		"v": "action:vsplit",
		"s": "action:split",
		"d": "expr:GetDeleteAction"
	},
	"list.insertMappings": {
		"<C-t>": "action:tabe",
		"<C-w>": "command:wincmd k"
	}
}
